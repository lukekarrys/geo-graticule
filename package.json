{
  "name": "geo-graticule",
  "description": "Helper for turning a geocode into information about its graticule.",
  "version": "2.0.3",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/geo-graticule/issues"
  },
  "dependencies": {
    "lodash": "^4.14.1",
    "node-geo-distance": "^1.2.0"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "babel-cli": "^6.5.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-tape-runner": "^2.0.1",
    "git-validate": "^2.1.4",
    "open": "0.0.5",
    "qs": "^6.2.1",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "homepage": "https://github.com/lukekarrys/geo-graticule",
  "keywords": [
    "geo",
    "geohash",
    "graticule",
    "xkcd"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lukekarrys/geo-graticule.git"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "lint": "standard",
    "map": "babel-node examples/map.js",
    "prepublish": "npm run compile",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "**/lib/**"
    ]
  }
}
